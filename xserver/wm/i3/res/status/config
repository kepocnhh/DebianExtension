general {
        colors = true
        interval = 5
}

order += "disk /"
order += "wireless _first_"
order += "tztime local"

disk "/" {
        format = "%avail/%total"
}

wireless _first_ {
        format_up = "%essid/%ip"
        format_down = "wireless down"
}

tztime local {
        format = "%Y/%m/%d %H:%M:%S"
}
