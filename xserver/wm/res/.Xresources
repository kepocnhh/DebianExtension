Xft.antialias: true
Xft.dpi: 100.0
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

URxvt.scrollBar: false
URxvt.font: xft:JetBrains Mono NL:style=Regular:size=10

URxvt.keysym.C-A-c: builtin-string:
URxvt.keysym.C-A-v: builtin-string:
URxvt.keysym.A-c: eval:selection_to_clipboard
URxvt.keysym.C-v: eval:paste_clipboard
